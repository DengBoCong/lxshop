<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dbc.lxshop.Model.Entity.LGoodsCategoryJoinEntity" table="l_goods_category_join" schema="lxshop">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="goodsId">
            <column name="goods_id" sql-type="int(11)"/>
        </property>
        <property name="categoryId">
            <column name="category_id" sql-type="int(11)"/>
        </property>
        <property name="addTime">
            <column name="add_time" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>