<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dbc.lxshop.Model.Entity.LRolePowerEntity" table="l_role_power" schema="lxshop">
        <id name="id">
            <column name="id" sql-type="int(11) unsigned"/>
        </id>
        <property name="roleId">
            <column name="role_id" sql-type="int(11)"/>
        </property>
        <property name="powerId">
            <column name="power_id" sql-type="int(11)"/>
        </property>
        <property name="addTime">
            <column name="add_time" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>