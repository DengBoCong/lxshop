<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dbc.lxshop.Model.Entity.LUserIntegralLogEntity" table="l_user_integral_log" schema="lxshop">
        <id name="id">
            <column name="id" sql-type="int(11) unsigned"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="type">
            <column name="type" sql-type="tinyint(1)"/>
        </property>
        <property name="originalIntegral">
            <column name="original_integral" sql-type="int(11)"/>
        </property>
        <property name="newIntegral">
            <column name="new_integral" sql-type="int(11)"/>
        </property>
        <property name="msg">
            <column name="msg" sql-type="varchar(500)" length="500"/>
        </property>
        <property name="operationId">
            <column name="operation_id" sql-type="int(11)"/>
        </property>
        <property name="addTime">
            <column name="add_time" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>