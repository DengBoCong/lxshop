<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dbc.lxshop.Model.Entity.LActivityPlanEntity" table="l_activity_plan" schema="lxshop">
        <id name="id">
            <column name="id" sql-type="int(11) unsigned" not-null="true"/>
        </id>
        <property name="description">
            <column name="description" sql-type="mediumtext" not-null="true"/>
        </property>
        <property name="url">
            <column name="url" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
        <property name="addTime">
            <column name="add_time" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="title">
            <column name="title" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="areaId">
            <column name="area_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>