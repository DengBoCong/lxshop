<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dbc.lxshop.Model.Entity.LSalesmanUserEntity" table="l_salesman_user" schema="lxshop">
        <id name="id">
            <column name="id" sql-type="int(11) unsigned"/>
        </id>
        <property name="pid">
            <column name="pid" sql-type="int(11)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="mobile">
            <column name="mobile" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="pwd">
            <column name="pwd" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="areaId">
            <column name="area_id" sql-type="int(11)"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(60)" length="60"/>
        </property>
        <property name="image">
            <column name="image" sql-type="varchar(1000)" length="1000"/>
        </property>
        <property name="idCard">
            <column name="id_card" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="idcardFphoto">
            <column name="idcard_fphoto" sql-type="varchar(1000)" length="1000"/>
        </property>
        <property name="idcardBphoto">
            <column name="idcard_bphoto" sql-type="varchar(1000)" length="1000"/>
        </property>
        <property name="lowerCount">
            <column name="lower_count" sql-type="int(11)"/>
        </property>
        <property name="addTime">
            <column name="add_time" sql-type="int(11)"/>
        </property>
        <property name="updTime">
            <column name="upd_time" sql-type="int(11)"/>
        </property>
        <property name="status">
            <column name="status" sql-type="tinyint(1)"/>
        </property>
        <property name="province">
            <column name="province" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="city">
            <column name="city" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(1000)" length="1000"/>
        </property>
        <property name="kind">
            <column name="kind" sql-type="tinyint(1)"/>
        </property>
    </class>
</hibernate-mapping>