<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dbc.lxshop.Model.Entity.LDefineViewEntity" table="l_define_view" schema="lxshop">
        <id name="id">
            <column name="id" sql-type="int(11) unsigned"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="content">
            <column name="content" sql-type="text"/>
        </property>
        <property name="isEnable">
            <column name="is_enable" sql-type="tinyint(1)"/>
        </property>
        <property name="kind">
            <column name="kind" sql-type="tinyint(1)"/>
        </property>
        <property name="url">
            <column name="url" sql-type="varchar(1000)" length="1000"/>
        </property>
        <property name="accessCount">
            <column name="access_count" sql-type="int(11)"/>
        </property>
        <property name="addTime">
            <column name="add_time" sql-type="int(11)"/>
        </property>
        <property name="updTime">
            <column name="upd_time" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>