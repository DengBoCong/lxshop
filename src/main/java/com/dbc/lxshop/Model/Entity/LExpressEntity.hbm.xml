<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dbc.lxshop.Model.Entity.LExpressEntity" table="l_express" schema="lxshop">
        <id name="id">
            <column name="id" sql-type="int(11) unsigned"/>
        </id>
        <property name="pid">
            <column name="pid" sql-type="int(11) unsigned"/>
        </property>
        <property name="icon">
            <column name="icon" sql-type="varchar(255)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="isEnable">
            <column name="is_enable" sql-type="tinyint(1) unsigned"/>
        </property>
        <property name="sort">
            <column name="sort" sql-type="tinyint(3) unsigned"/>
        </property>
        <property name="addTime">
            <column name="add_time" sql-type="int(11) unsigned"/>
        </property>
        <property name="updTime">
            <column name="upd_time" sql-type="int(11) unsigned"/>
        </property>
    </class>
</hibernate-mapping>